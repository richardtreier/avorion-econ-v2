{
  "name": "avorion-econ-svelte",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn parse-game-files",
    "start": "yarn dev",
    "dev": "yarn parse-game-files && svelte-kit dev",
    "build": "yarn parse-game-files && svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format-all": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "parse-game-files": "node avorion/import-data-from-game-files.cjs"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "run-s": "^0.0.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^2.2.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
