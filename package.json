{
  "name": "asd",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run parse-game-files && vite dev",
    "build": "npm run parse-game-files && vite build",
    "preview": "npm run parse-game-files && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "parse-game-files": "node avorion/import-data-from-game-files.cjs"
